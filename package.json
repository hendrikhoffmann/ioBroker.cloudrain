{
  "name": "iobroker.cloudrain",
  "version": "0.2.3",
  "description": "This Adapter connects iobroker to the Cloudrain Smart Irrigation System",
  "author": {
    "name": "hendrikhoffmann",
    "email": "hhoffmann@web.de"
  },
  "homepage": "https://github.com/hendrikhoffmann/ioBroker.cloudrain",
  "license": "MIT",
  "keywords": [
    "cloudrain",
    "irrigation",
    "smart",
    "smartgarden"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hendrikhoffmann/ioBroker.cloudrain"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.8",
    "@iobroker/adapter-react": "2.2.4",
    "@iobroker/dev-server": "^0.6.0",
    "@material-ui/core": "^4.12.4",
    "got": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@iobroker/testing": "^4.1.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/gulp": "^4.0.11",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "parcel-bundler": "^1.12.5",
    "proxyquire": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0",
    "typescript": "~5.1.3"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "watch:parcel": "parcel admin/src/index.jsx -d admin/build",
    "build:parcel": "parcel build admin/src/index.jsx -d admin/build",
    "build": "npm run build:parcel",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint --ext .js,.jsx",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/hendrikhoffmann/ioBroker.cloudrain/issues"
  },
  "readmeFilename": "README.md"
}
